{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/$1",
            "status": 404
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}